# - Config file for the inria_wbc_teleop package
# It defines the following variables
#  INRIA_WBC_com_INCLUDE_DIRS - include directories for inria_wbc_com
#  INRIA_WBC_COM_LIBRARY_DIRS - libraries to link against
@PACKAGE_INIT@
include(CMakeFindDependencyMacro)

find_package(inria_wbc REQUIRED)
find_package(xsens_parser REQUIRED)

set(CMAKE_CXX_FLAGS "${CMAKE_CXX_FLAGS} -DBOOST_MATH_DISABLE_FLOAT128 -std=c++14")

include("${CMAKE_CURRENT_LIST_DIR}/inria_wbc_comTargets.cmake")

set_and_check(INRIA_WBC_COM_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(INRIA_WBC_COM_LIBRARY_DIRS "@PACKAGE_LIBRARY_INSTALL_DIR@")


set(INRIA_WBC_COM_LIBRARIES "@LIBRARY@")
set(INRIA_WBC_COM_DEFINITIONS "@DEFINITIONS@")
set(INRIA_WBC_COM_DEPENDENCIES_LIBS "@DEPENDENCIES_LIBS@")
set(INRIA_WBC_COM_DEPENDENCIES_INCS "@DEPENDENCIES_INCS@")


